{
  "name": "server",
  "version": "0.0.0",
  "description": "Server for password vault",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsnd --exit-child --transpile-only src/main.ts",
    "start": "cross-env NODE_ENV=production node dist/src/main.js",
    "start:test": "cross-env NODE_ENV=test node dist/src/main.js",
    "build": "tsc",
    "test": "cross-env NODE_ENV=test vitest run",
    "lint": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix"
  },
  "keywords": [
    "fastify"
  ],
  "author": "Juan Carlos Lucio",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.5",
    "cross-env": "^7.0.3",
    "eslint": "8.26.0",
    "eslint-config-custom": "*",
    "pino-pretty": "^9.1.1",
    "supertest": "^6.3.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "@fastify/cors": "^8.1.1",
    "@fastify/helmet": "^10.0.2",
    "@fastify/jwt": "^6.3.2",
    "@typegoose/typegoose": "^9.12.1",
    "argon2": "^0.30.1",
    "dotenv": "^16.0.3",
    "fastify": "^4.9.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.7.0",
    "pino": "^8.7.0"
  }
}
